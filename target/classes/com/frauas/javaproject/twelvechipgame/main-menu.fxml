<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.frauas.javaproject.twelvechipgame.controller.MainMenuController">
   <children>
      <Label alignment="CENTER" layoutY="14.0" prefHeight="54.0" prefWidth="600.0" text="Play against" textAlignment="CENTER">
         <font>
            <Font size="28.0" />
         </font></Label>
      <Button fx:id="threePlayerButton" layoutX="254.0" layoutY="84.0" mnemonicParsing="false" onAction="#onThreePlayerButtonClicked" prefHeight="39.0" prefWidth="92.0" text="3 Players" />
      <Button fx:id="fourPlayerButton" layoutX="254.0" layoutY="130.0" mnemonicParsing="false" onAction="#onFourPlayerButtonClicked" prefHeight="39.0" prefWidth="92.0" text="4 Players" />
      <Button fx:id="settingsButton" layoutX="254.0" layoutY="294.0" mnemonicParsing="false" onAction="#switchToSettings" prefHeight="39.0" prefWidth="92.0" text="Settings" />
      <Button fx:id="exitButton" layoutX="254.0" layoutY="346.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="39.0" prefWidth="92.0" text="Exit to Desktop" />
      <TextArea fx:id="debugField" layoutX="418.0" layoutY="221.0" prefHeight="166.0" prefWidth="168.0" />
      <Button layoutX="254.0" layoutY="239.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="92.0" text="Game Rules" />
   </children>
</AnchorPane>
